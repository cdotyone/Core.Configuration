{
  "release": {
    "branch": "master",
    "dryRun": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "semantic-release-env",
        { "setOnlyOnRelease": false }
      ],
      [
        "@semantic-release/npm",
        { "npmPublish": false }
      ]
    ]
  }
}
